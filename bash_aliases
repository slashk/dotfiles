# SCM aliases
alias g='git '
alias gs='git status'
alias gb='git branch'
alias gc='git commit'
alias gt='git log --abbrev-commit --pretty="%ai %s (%ae #%h)" -n 10'
alias gta='git log --abbrev-commit --pretty="%ai %s (%ae #%h)"'
alias twt='twitter '
alias h=history
alias b='bzr '
alias bs='bzr status'
alias bt='bzr top'

# apt-get updates
alias up='sudo apt-get update; sudo apt-get dist-upgrade'
